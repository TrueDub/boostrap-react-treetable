{
  "name": "bootstrap-react-treetable",
  "version": "3.0.0",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/truedub/bootstrap-react-treetable.git"
  },
  "homepage": "https://github.com/truedub/bootstrap-react-treetable",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "start": "react-scripts start",
    "build": "npm run clean && npm run lint && babel src/lib --out-dir dist && copyfiles -f src/lib/*.css dist",
    "test": "cross-env CI=true react-scripts test --env=jsdom --coverage",
    "eject": "react-scripts eject",
    "clean:demo": "rimraf build",
    "build:demo": "npm run clean:demo && webpack --config webpack.config.demo.js -p --bail",
    "lint": "eslint src"
  },
  "jest": {
    "coverageReporters": [
      "html"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm test",
      "pre-push": "npm run lint && npm test"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "7.12.1",
    "@fortawesome/fontawesome-svg-core": "6.1.1",
    "@fortawesome/free-solid-svg-icons": "6.1.1",
    "@fortawesome/react-fontawesome": "0.1.18",
    "core-js": "3.22.2",
    "date-fns": "2.28.0",
    "html-react-parser": "1.4.12",
    "prop-types": "15.8.1"
  },
  "devDependencies": {
    "@babel/cli": "7.17.6",
    "@babel/core": "7.17.9",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "bootstrap": "4.4.1",
    "copyfiles": "2.4.1",
    "cross-env": "7.0.3",
    "del": "6.0.0",
    "enzyme": "3.11.0",
    "@wojtekmaj/enzyme-adapter-react-17": "0.6.7",
    "eslint-plugin-react-hooks": "4.4.0",
    "highlight.js": "11.5.1",
    "husky": "7.0.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-highlight": "0.14.0",
    "react-scripts": "5.0.1",
    "react-test-renderer": "17.0.2",
    "rimraf": "3.0.2",
    "webpack-cli": "4.9.2"
  },
  "peerDependencies": {
    "bootstrap": "^4.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "browserslist": {
