    "@fortawesome/react-fontawesome": "0.1.8",
    "core-js": "^3.8.3",
    "date-fns": "^2.16.1",
    "prop-types": "15.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "7.8.4",
    "@babel/preset-env": "7.8.4",
    "@babel/preset-react": "7.8.3",
    "bootstrap": "4.4.1",
    "copyfiles": "2.2.0",
    "cross-env": "7.0.0",
    "del": "5.1.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "highlight.js": "10.5.0",
    "husky": "4.2.3",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-highlight": "^0.12.0",
    "react-scripts": "4.0.1",
    "react-test-renderer": "16.12.0",
    "rimraf": "3.0.2",
    "webpack-cli": "^3.3.12"
  },
  "peerDependencies": {
    "bootstrap": "^4.4.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
